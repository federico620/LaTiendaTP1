@model IEnumerable<LaTienda.StockSet>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cantidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ColorSet.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProductoSet.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SucursalSet.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TalleSet.Descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cantidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ColorSet.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProductoSet.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SucursalSet.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TalleSet.Descripcion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
